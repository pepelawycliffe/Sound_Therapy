# https://EditorConfig.org
root = true

[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8
indent_style = space
indent_size = 2
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false

# weblate doesn't respect editorconfig.
[strings.xml]
indent_size = 4
insert_final_newline = false
